    {
      "id": "concept-content",
      "type": "text",
      "x": 70,
      "y": 490,
      "width": 490,
      "height": 130,
      "text": "@Entity        → \"This class represents a database table\"\n@Table         → \"Specify the table name\"\n@Id            → \"Primary key\"\n@Column        → \"Map to database column\"\n@OneToMany     → \"One order has many items\" (parent side)\n@ManyToOne     → \"Many items belong to one order\" (child side)\n@JoinColumn    → \"Foreign key column name\"\n@GeneratedValue → \"Auto-generate ID (UUID)\"",
      "fontSize": 11,
      "fontFamily": 1,
      "textAlign": "left",
      "strokeColor": "#495057",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 1,
      "roughness": 1,
      "opacity": 100,
      "seed": 27
    },
